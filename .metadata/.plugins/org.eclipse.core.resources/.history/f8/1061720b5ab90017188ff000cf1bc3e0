package oop;

public class BankAccCls {
	String AccountNumber;
	String RoutingNumber;
	String Name;
	String SSN;
	String AccountType;
	
	//Constructor Definitions - mUnique Methods
		//1.) They are used to define / setup / initialize properties of an object
		//2.) Constructors are Implicitly called upon instantiation
		//3.) The same name as the class itself
	BankAccCls(){
		System.out.println("account created");
		
	}
		//overloading; call same method name with different argument
	BankAccCls(String AccountType){
		System.out.println("Thanks for opening acc type:" + AccountType);
	}
	
	BankAccCls(String AccountType, Double DepositAmount){
		System.out.println("Thanks for opening acc type:" + AccountType );
		System.out.println(" with deposit of " + DepositAmount);
	}
	
	//Define Methos
	void deposit(){
		
	}
	
	void Withdraw(){
		
	}
	
	void CheckBalance(){
		
	}
	
	void GetStatus(){
		
	}
	

}
